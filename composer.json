{
    "name": "jorgegc/behat-drupal-extension",
    "description": "The commonly used functionality used in Behat testing using Drupal.",
    "keywords": [
        "behat",
        "drupal"
    ],
    "homepage": "https://github.com/jorgegc/behat-drupal-extension",
    "license": "GPL-2.0+",
    "support": {
        "issues": "https://github.com/jorgegc/behat-drupal-extension",
        "source": "https://github.com/jorgegc/behat-drupal-extension"
    },
    "require": {
        "drupal/drupal-extension": "~3.0"
    },
    "autoload": {
        "psr-0": {
            "Drupal\\DrupalExtension\\Context": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
